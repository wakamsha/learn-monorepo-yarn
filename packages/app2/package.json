{
  "name": "@learn-monorepo-yarn/app2",
  "version": "1.0.0",
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "type-check": "tsc --noEmit",
    "typegen:scss": "typed-scss-modules src",
    "dev": "vite serve",
    "start": "run -T run-p \"type-check --watch\" \"typegen:scss -w\" \"dev\""
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "3.1.0",
    "sass": "1.58.0",
    "typed-scss-modules": "7.0.2",
    "vite": "4.1.1"
  }
}
